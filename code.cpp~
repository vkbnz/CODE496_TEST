

#include <iostream> //cout
using namespace std; //std = standard

int main(){

  // cout << "Hello World\n"; // \n = newline


  int nx = 10;
  int ny = 20;
  double phi[nx];
  double phi2[nx][ny];
  
  
  for(int i = 0; i <= nx-1; i++){
    phi[i] = 1.0;
  }

  for(int i = 0; i <= nx-1; i++){
    cout << "phi[" << i << "] = " << phi[i] << "\n";
  }

  for(int i = 0; i <= nx-1; i++){
    for(int j = 0; j <= ny-1; j++){
      phi2[i][j] = i*j;
    }
  }
  
}
